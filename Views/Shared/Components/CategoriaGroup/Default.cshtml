<div class="list-group">
    <button type="button" class="list-group-item list-group-item-action active bg-dark text-white">
        <b>Skins</b>
    </button>
    <button type="button" class="list-group-item list-group-item-action">
        @foreach(var categoria in Model)
        {
            <a class="dropdown-item" asp-controller="Skin" asp-action="List" style="color:black" 
            asp-route-categoria="@categoria.CategoriaNome">@categoria.CategoriaNome</a>
        }
    </button>
</div>